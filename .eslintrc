{
  "parser": "babel-eslint",
  "extends": "atomikui",
  "rules": {
    "max-len": 0,
    "import/no-extraneous-dependencies": 0
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["@atomikui-core", "./node_modules/@atomikui/core/dist/components"]
        ]
      }
    }
  }
}
